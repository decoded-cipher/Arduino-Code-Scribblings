void setup() {
  Serial.begin(115200);

  if(!display.begin(SSD1306_SWITCHCAPVCC, 0x3C)) {
    Serial.println(F("SSD1306 allocation failed"));
    for(;;);
  }
  delay(1000);
  display.clearDisplay(); //All pixels are off
}

void blink_eye(){
  display.clearDisplay();
  display.drawBitmap(0,0,opened_eye,128,64,WHITE);
  display.display();
 
  display.clearDisplay();
  display.drawBitmap(0,0,semiopened_eye,128,64,WHITE);
  display.display();
 
  display.clearDisplay();
  display.drawBitmap(0,0,closed_eye,128,64,WHITE);
  display.display();

  display.clearDisplay();
  display.drawBitmap(0,0,semiopened_eye,128,64,WHITE);
  display.display();
  
  display.clearDisplay();
  display.drawBitmap(0,0,opened_eye,128,64,WHITE);
  display.display();
  delay(1000);
}

